{
    "title": "PumpkinDB",
    "plugins": ["theme-api", "fontsettings", "github-buttons", "expandable-chapters"],
    "pluginsConfig": {
      "theme-api": {
         "split": true,
         "theme": "dark"
      },
      "fontsettings": {
        "theme": "white",
        "family": "serif",
        "size": 3
      },
      "github-buttons": {
        "repo": "PumpkinDB/PumpkinDB",
        "types": [
          "star",
          "watch"
        ],
        "size": "large"
      }
    }
}
